<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.shop.mapper.ShopMapper">

	<select id="detail">
		select *
		  from ECOMMERCE
		 where EC_ID = #{ecId}
	</select>
	
	<insert id="createProduct">
		insert into ECOMMERCE(EC_ID, EC_NAME, EC_COLOR, EC_DETAIL)
		values (#{ecId},#{ecName},#{ecColor},#{ecDetail})
	</insert>

</mapper>